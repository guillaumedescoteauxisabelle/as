#git
alias gs="git status"
alias gsp="gs ."
alias gd="git diff "
alias gdb="git diff -b "
alias gdp="gdb ."
add_commit_push() { 
	local _msg="$1";shift 1;f="$1";if [ "$f" == "" ]; then f=".";else f="";fi;git add $f $@ &> /dev/null ; git commit $f $@ -m "$_msg"; git push &>/dev/null
}
alias gcp="add_commit_push "
alias gcln="git commit . -m clean"

