## --@STCGoal One operation CLI to edit document from anywhere(OS Context)
e() {
	local _ecmd="code "
	if [ "$(uname)" == "Linux" ];then _ecmd="explorer.exe ";fi #Yes on WSL you can interface with Windows explorer 
	export ecmd="$_ecmd"
	# Run the editing
	echo "Opening $1 :)"
	$ecmd "$1"
	}

#alias e="e"

