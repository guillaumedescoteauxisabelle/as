#!/usr/bin/env bash

#@STCGoal Autocomplete the Docker Container Execution of Bash for active containers

#complete -W "$(lpd)" dkcrm
_dkbash_completions()
{
	#. $binroot/fndkRefreshRunningContainers.sh
	dkrunningcontainers=$(cd $rwroot;./_adm__stopRemove_Containers__210502.sh --list | tr "\n" " " )
	COMPREPLY=($(compgen -W "$dkrunningcontainers" "${COMP_WORDS[1]}"))
  #echo " "
}

complete -F _dkbash_completions dkbash

